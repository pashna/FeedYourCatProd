<fest:template xmlns:fest="http://fest.mail.ru"
    context_name="json">
    <div class="game-title">
        Feed Your Cat!
        <img src="img/logo.png" width="30" alt="" class="game-title__img" />            
    </div>
    <div class="content content_padding_xs">
        <div class="content__title">Score Board</div>
        <div class="reload-menu" id="reload_scoreboard">
            <div class="lose-menu__text">Load error</div>
            <div class="toolbar__button_xs"><img src="img/reload.png" class="reload__picture"/></div>
        </div>
        <div id="scoreTable"></div>
        <div class="toolbar toolbar_padding_xs">
            <a href="#" class="toolbar__button">Back</a>
        </div>
    </div>
</fest:template>